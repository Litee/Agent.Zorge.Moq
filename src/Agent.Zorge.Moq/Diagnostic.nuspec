<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Agent.Zorge.Moq</id>
    <version>1.0.0.0</version>
    <title>Agent.Zorge.Moq</title>
    <authors>Andrey "Litee" Lipatkin</authors>
    <owners>Andrey "Litee" Lipatkin</owners>
    <licenseUrl>https://github.com/Litee/Agent.Zorge.Moq/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Litee/Agent.Zorge.Moq</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Visual Studio extension and Roslyn analyzer that helps to write unit tests using Moq mocking library. Port of Resharper extension to Roslyn.</description>
    <releaseNotes>First release: Highlights for bad callbacks, It.IsAny() is offered in completion popup.</releaseNotes>
    <copyright>2015-2017 Andrey Lipatkin</copyright>
    <tags>moq, mock, test, analyzer</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>